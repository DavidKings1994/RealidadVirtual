{
    "name": "Serial Test",
    "version": "1",
    "manifest_version": 2,
    "permissions": ["serial"],
    "minimum_chrome_version": "23",
    "icons": {
        "16": "img/logo_16.png",
        "128": "img/logo_128.png"
    },
    "app": {
        "background": {
            "scripts": ["launch.js"],
            "transient": true
        }
    },
    "externally_connectable": {
        "matches": ["*://localhost/*"]
    }
}
